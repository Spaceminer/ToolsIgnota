<Window
    x:Class="ToolsIgnota.UI.Windows.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc="using:ToolsIgnota.UI.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <NavigationView
        x:Name="navMenu"
        SelectionChanged="NavMenu_SelectionChanged"
        IsSettingsVisible="False">
        <NavigationView.MenuItems>
            <NavigationViewItem Icon="People" Content="Initiative Display" Tag="InitiativeControlPage" />
        </NavigationView.MenuItems>
        <Frame x:Name="contentFrame" Padding="10"/>
    </NavigationView>
</Window>
